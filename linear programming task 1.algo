FUNCTION dot_product (V1:array of FLOAT[2], V2:array of FLOAT[2]) : FLOAT.
VAR
 ps:FLOAT
    
BEGIN
    ps=v1[1]*V2[1]+ v1[2]*V2[2];
    RETURN ps ;
END

